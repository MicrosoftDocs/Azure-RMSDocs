{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
        {
          "files": [
          "**/images/**", 
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html"
          ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "Azure-RMSDocs",
    "template": "docs.html",
    "globalMetadata": {
      "layout": "Conceptual",
      "breadcrumb_path": "/enterprise-mobility-security/toc.json",
      "searchScope": ["Information Protection"]
    }
  }
}

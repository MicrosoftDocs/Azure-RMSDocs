{
  "build": {
    "content": [
      {
        "files": [
          "**/**.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/images/**",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.html"
        ],
        "exclude": [
          "**/obj/**",
          "_themes/**"
        ]
      }
    ],
    "dest": "Azure-RMSDocs",
    "template": "docs.html",
    "globalMetadata": {
      "feedback_system": "GitHub",
      "uhfHeaderId": "azure",
      "feedback_github_repo": "MicrosoftDocs/azure-rmsdocs",
      "feedback_product_url": "https://feedbackportal.microsoft.com/feedback/",
      "layout": "Conceptual",
      "breadcrumb_path": "/azure/information-protection/bread/toc.json",
      "searchScope": [
        "Information Protection"
      ]
    },
    "markdownEngineName": "markdig"
  }
}
